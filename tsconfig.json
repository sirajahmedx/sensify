{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": false,
    "allowJs": true,
    "jsx": "react-native",
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",                              
    "**/*.tsx",                             
    ".expo/types/**/*.ts",                  
    "expo-env.d.ts",                        
    "app/(tabs)/smile-detector.rsx"        
, "app/(tabs)/smile-capturer.jsx"  ],
   "exclude": ["node_modules"]
}
